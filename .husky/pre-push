echo "\n Checking Branch Name ‚è≥"

BRANCH=`git rev-parse --abbrev-ref HEAD`
PROTECTED_BRANCH_NAME="production"

if [ "$BRANCH" == $PROTECTED_BRANCH_NAME ]; 
then
	echo "\n üî¥ You cannot push to remote $PROTECTED_BRANCH_NAME branch. Please create your own branch and raise PR. ‚õîÔ∏è" && exit 1
fi

echo "\n ‚úÖ Branch Name Check Passed ‚úÖ"

exit 0
